  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@actions/exec": "^1.1.1",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@vercel/ncc": "^0.38.1",
    "rimraf": "^6.0.0",
    "typescript": "^5.6.3"
  }
}